{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "signalRConnectionInfo",
      "name": "connection",
      "userId": "{query.userid}",
      "hubName": "chat",
      "connectionStringSetting": "SignalRConnectionString",
      "direction": "in"
    }
  ]
}
